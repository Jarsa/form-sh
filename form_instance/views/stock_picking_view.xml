<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_picking_form_custom" model="ir.ui.view">
        <field name="name">view.picking.form.custom</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_line_ids_without_package']/tree/field[@name='product_id']" position="after">
                <field name="product_supplier_name"/>
            </xpath>
             <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="after">
                <field name="product_supplier_name"/>
            </xpath>
        </field>
    </record>
</odoo>
