<odoo>
    <record id="cost_margin_utility_report_tree_view" model="ir.ui.view">
        <field name="name">cost.margin.utility.report.tree</field>
        <field name="model">cost.margin.utility.report</field>
        <field name="arch" type="xml">
            <tree string="Cost Margin Utility">
                <field name="reference"/>
                <field name="product_category_id"/>
                <field name="default_code"/>
                <field name="form_id"/>
                <field name="product_id"/>
                <field name="product_description"/>
                <field name="sold_qty"/>
                <field name="total_sale"/>
                <field name="total_cost"/>
                <field name="unit_cost"/>
                <field name="contribution"/>
                <field name="margin"/>
            </tree>
        </field>
    </record>
</odoo>
