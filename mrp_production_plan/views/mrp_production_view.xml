<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="mrp_production_form_view" model="ir.ui.view">
        <field name="name">mrp.production.form - mrp_production_plan</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id"
               ref="mrp_request.mrp_production_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='mrp_request_id']" position="after">
                <field name="plan_id"/>
            </xpath>
        </field>
    </record>
</odoo>
