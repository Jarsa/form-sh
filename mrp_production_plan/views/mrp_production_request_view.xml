<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="mrp_production_request_form_view" model="ir.ui.view">
        <field name="name">mrp.production.request.form - mrp_production_plan</field>
        <field name="model">mrp.production.request</field>
        <field name="inherit_id"
               ref="mrp_production_request.view_mrp_production_request_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="plan_id"/>
            </xpath>
        </field>
    </record>
</odoo>
